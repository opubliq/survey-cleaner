{
  "workflow_id": "EuQL3RwAz5ULPxjP",
  "name": "survey-cleaner",
  "description": "MVP: Simple n8n workflow to receive file uploads via webhook",
  "webhook_endpoint": "/webhook/survey-cleaner",
  "active": true,
  "flow": {
    "trigger": {
      "type": "webhook",
      "method": "POST",
      "path": "survey-cleaner",
      "accepts": ["multipart/form-data"],
      "binary_data": true
    },
    "processing": [
      {
        "step": "receive_files",
        "description": "Receive and acknowledge file upload",
        "response": {
          "success": true,
          "message": "Files received successfully",
          "timestamp": "{{new Date().toISOString()}}"
        }
      }
    ]
  },
  "conventions": {
    "demographic_prefix": "demo_",
    "opinion_prefix": "op_",
    "missing_values": "NA",
    "scale_standardization": "0-1 for opinion variables"
  },
  "supported_formats": {
    "survey_files": ["csv", "sav", "xlsx"],
    "codebook_files": ["pdf", "xlsx", "csv", "txt"]
  },
  "usage": {
    "curl_example": "curl -X POST http://localhost:5678/webhook/survey-cleaner -F 'survey_file=@data.csv' -F 'codebook_file=@codebook.pdf'",
    "response_format": "JSON with generated Python script and metadata"
  }
}